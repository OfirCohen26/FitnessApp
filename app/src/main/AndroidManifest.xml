<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitnessapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="false"
        android:theme="@style/Theme.FitnessApplication">
        <activity
            android:name=".Screen.Finished_Plan"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Screen.Show_Step"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Screen.Plan_Description" />
        <activity
            android:name=".Screen.Plans"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Auth.Sign_Up"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".Auth.Sign_In"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".Screen.Activity_Splash"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Auth.Profile"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>